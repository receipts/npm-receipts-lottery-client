{
  "main-request": {
    "url": "https://loteriaparagonowa.gov.pl",
    "timeout": 5000,
    "headers": {
      "User-Agent": "loteriaparagonowa",
      "Origin": "loteriaparagonowa",
      "Referer": "loteriaparagonowa",
      "X-Requested-With": "XMLHttpRequest"
    }
  },
  "tickets-request": {
    "url": "https://loteriaparagonowa.gov.pl/paragon/stworz",
    "timeout": 5000,
    "headers": {
      "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8",
      "User-Agent": "loteriaparagonowa",
      "Origin": "loteriaparagonowa",
      "Referer": "loteriaparagonowa",
      "X-Requested-With": "XMLHttpRequest"
    },
    "fields": [
      {
        "fieldName": "x-csrf-token",
        "fieldRegex": "/name=\"_token\" id=\"csrf-token\" value=\"(.+?)\"/g",
        "fieldDefaultValue": null,
        "createReceiptRequestField": null
      },
      {
        "fieldName": "captcha",
        "fieldRegex": "/Zadanie matematyczne: (.+?) daje:/g",
        "fieldDefaultValue": null,
        "createReceiptRequestField": null
      }
    ]
  },
  "auth-request": {
    "url": "https://loteriaparagonowa.gov.pl/auth/login",
    "timeout": 5000,
    "headers": {
      "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8",
      "User-Agent": "loteriaparagonowa",
      "Origin": "loteriaparagonowa",
      "Referer": "loteriaparagonowa",
      "X-Requested-With": "XMLHttpRequest"
    }
  },
  "account-request": {
    "url": "https://loteriaparagonowa.gov.pl/moje-konto",
    "timeout": 5000,
    "headers": {
      "User-Agent": "loteriaparagonowa",
      "Referer": "loteriaparagonowa",
      "upgrade-insecure-requests": "1"
    }
  },
  "captcha-request": {
    "url": "https://loteriaparagonowa.gov.pl/validation/captcha",
    "timeout": 5000,
    "headers": {
      "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8",
      "User-Agent": "loteriaparagonowa",
      "Origin": "loteriaparagonowa",
      "Referer": "loteriaparagonowa",
      "X-Requested-With": "XMLHttpRequest"
    }
  }
}